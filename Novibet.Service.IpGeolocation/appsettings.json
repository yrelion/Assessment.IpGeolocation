{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;database=NovibetGeolocation;trusted_connection=true;MultipleActiveResultSets=True"
  },
  "Proxy": {
    "IPStack": {
      "ServiceId": "service",
      "ServiceName": "service",
      "BaseUrl": "http://localhost",
      "ContentType": "application/json",
      "AccessKey": "key"
    }
  },
  "Worker": {
    "GeolocationBatchUpdate": {
      "Interval": 5000,
      "JobBatchSize": 2,
      "JobItemBatchSize": 10
    }
  },
  "Cache": {
    "InMemory": {
      "AbsoluteExpirationMinutes": 1
    }
  } 
}
